  },
  "devDependencies": {
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "mocha": "^10.7.3",
    "istanbul": "^0.4.5",
    "semistandard": "^17.0.0",
    "eslint": "^9.13.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "proxyquire": "^2.1.3"
